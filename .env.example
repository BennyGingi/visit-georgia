# ===========================================
# Visit Georgia - Environment Variables
# ===========================================
# Copy this file to .env.local and fill in values.
# For Vercel: add these in Project Settings > Environment Variables.

# --- EmailJS (optional - enables email booking) ---
# Sign up at https://www.emailjs.com/ and create a service + template.
# Without these, the "Send via Email" button is disabled; WhatsApp booking still works.
NEXT_PUBLIC_EMAILJS_SERVICE_ID=
NEXT_PUBLIC_EMAILJS_TEMPLATE_ID=
NEXT_PUBLIC_EMAILJS_PUBLIC_KEY=

# --- Contact email (optional - defaults to gingi2603@gmail.com) ---
NEXT_PUBLIC_CONTACT_EMAIL=
